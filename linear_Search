#include<iostream>
using namespace std;

void search(int array[],int size,int key){
	int i,cnt=0;
	for(i=0;i<size;i++){
		if(key==array[i]){
			cout<<"Element found at location:  "<<i<<endl;
			cnt=cnt+1;
		}
	}
	if(cnt==0){
		cout<<"Element not found ";
	}
	
}
int main()
{
int ar[]={12,4,5,9,12,4,8,4};	
search(ar,8,4);
}
//Why not to use bsearch instead. it work as follow.
int bsearch(){
int a[],size,low,high,mid,cnt;
cout<<"Enter the size of the array";
cin>>size;
}
